{
	"name": "d3.ru/leprosorium.ru service pack 3",
	"version": "3.0.65.3",
	"description": "Модульный сервис пак для сайтов d3.ru/leprosorium.ru",
	"manifest_version": 2,
	"content_scripts": 
	[
		{
			"matches": ["http://*.d3.ru/*", "http://*.leprosorium.ru/*"], 
			"js": ["d3.loader.user.js"],
			"run_at": "document_end"
		}
	],
	"permissions": 
	[
	    "storage", "tabs"
  	],
	"icons": 
	{
		"128": "i128.png"
	},
	"web_accessible_resources":
	[
		"d3.user.js"
	]
}
